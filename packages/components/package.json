{
  "name": "@clan/framework-components",
  "version": "1.0.0",
  "description": "React components library for BroClan Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css",
      "default": "./dist/styles/index.css"
    },
    "./styles/index.css": "./dist/styles/index.css",
    "./dist/styles/index.css": "./dist/styles/index.css"
  },
  "style": "./dist/styles/index.css",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rimraf dist",
    "preview": "vite preview",
    "test": "echo \"No tests specified\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "components",
    "wallet",
    "ui",
    "typescript"
  ],
  "author": "BroClan Team",
  "license": "MPL-2.0",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@clan/framework-core": "^1.0.0",
    "@clan/framework-providers": "^1.0.0",
    "clsx": "^2.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.20",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-svgr": "^4.3.0"
  }
}
